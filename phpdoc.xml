<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <version>3.0.0</version>
    <paths>
        <output>output</output>
        <cache>cache</cache>
    </paths>
    <parser>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformations>
        <template name="clean"/>
    </transformations>
    <files>
        <directory>.</directory>
        <ignore>vendor/</ignore>
    </files>
</phpdocumentor>
